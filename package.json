{
  "name": "@omrprks/jsonmerge",
  "version": "1.4.0",
  "private": false,
  "description": "Small tool for merging json files",
  "repository": "github:omrprks/jsonmerge.git",
  "author": "Omar Parkes (https://github.com/omrprks)",
  "license": "MIT",
  "bugs": "https://github.com/omrprks/jsonmerge/issues",
  "homepage": "https://github.com/omrprks/jsonmerge#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "jsonmerge": "./dist/index.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "npm run build -- --watch",
    "build": "ncc build src/index.ts -o dist --minify",
    "prepare": "npm run build",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/node": "^14.14.22",
    "@types/yargs": "^15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vercel/ncc": "^0.23.0",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "ts-node-dev": "^1.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "yargs": "^17.0.1"
  }
}
